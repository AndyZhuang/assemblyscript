{
  "name": "assemblyscript",
  "description": "A subset of TypeScript that compiles to WebAssembly.",
  "version": "0.0.1",
  "author": "Daniel Wirtz <dcode+assemblyscript@dcode.io>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcodeIO/AssemblyScript.git"
  },
  "bugs": {
    "url": "https://github.com/dcodeIO/AssemblyScript/issues"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "typescript",
    "compiler"
  ],
  "bin": {
    "asc": "bin/asc"
  },
  "typings": "assembly.d.ts",
  "dependencies": {
    "@protobufjs/base64": "^1.1.2",
    "@types/long": "^3.0.31",
    "@types/minimist": "^1.2.0",
    "@types/node": "^7.0.32",
    "binaryen": "^1.37.14",
    "long": "^3.2.0",
    "minimist": "^1.2.0",
    "performance-now": "^2.1.0",
    "ts-node": "^3.0.6",
    "typescript": "^2.4.0"
  },
  "devDependencies": {
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.3",
    "webassembly": "^0.11.0",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "build:malloc": "node scripts/build-malloc",
    "build:typescript": "tsc -P lib/typescript && echo export = ts; >> lib/typescript/build/index.d.ts",
    "build:loader": "cd lib/loader && webpack --config webpack.config.js && cd ../..",
    "build:alldeps": "node scripts/build-check && npm run build:loader && npm run build:malloc && npm run build:typescript",
    "build:library": "node scripts/build-library",
    "build:bundle": "webpack --config webpack.config.js",
    "build": "npm run lint && npm run build:library && npm run build:bundle",
    "lint": "tslint --config tslint.json src/**/*.ts cli/**/*.ts assembly.d.ts",
    "test": "node bin/asc tests/test.ts"
  },
  "browser": {
    "buffer": false,
    "crypto": false,
    "fs": false,
    "os": false,
    "source-map-support": false
  }
}
