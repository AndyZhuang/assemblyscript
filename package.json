{
  "name": "assemblyscript",
  "description": "A subset of TypeScript that compiles to WebAssembly.",
  "version": "0.0.1",
  "author": "Daniel Wirtz <dcode+assemblyscript@dcode.io>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcodeIO/AssemblyScript.git"
  },
  "bugs": {
    "url": "https://github.com/dcodeIO/AssemblyScript/issues"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "typescript",
    "compiler"
  ],
  "bin": {
    "asc": "bin/asc"
  },
  "typings": "assembly.d.ts",
  "dependencies": {
    "@protobufjs/base64": "^1.1.1",
    "@types/long": "^3.0.31",
    "@types/minimist": "^1.2.0",
    "@types/node": "^7.0.27",
    "binaryen": "^1.37.13",
    "long": "^3.2.0",
    "minimist": "^1.2.0",
    "performance-now": "^2.1.0",
    "ts-node": "^3.0.4",
    "typescript": "^2.4.0-dev.20170604"
  },
  "scripts": {
    "build:malloc": "node scripts/build-malloc",
    "build:typescript": "tsc -P lib/typescript && echo export = ts; >> lib/typescript/build/index.d.ts",
    "build:library": "node scripts/build-library",
    "build:bundle": "webpack --config webpack.config.js --display-max-modules 100",
    "build:dist": "uglifyjs --compress --mangle --output dist/assemblyscript.min.js --source-map root=dist/assemblyscript.js.map,url=assemblyscript.min.js.map -- dist/assemblyscript.js",
    "build": "npm run build:library && npm run build:bundle && npm run build:dist",
    "lint": "tslint --config tslint.json src/**/*.ts cli/**/*.ts assembly.d.ts",
    "test": "node bin/asc tests/test.ts"
  },
  "devDependencies": {
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.2",
    "tslint-eslint-rules": "^4.1.1",
    "uglify-js": "^3.0.15",
    "webassembly": "^0.11.0",
    "webpack": "^2.6.1"
  },
  "browser": {
    "buffer": false,
    "crypto": false,
    "fs": false,
    "os": false,
    "source-map-support": false
  },
  "false": {}
}
